#include <iostream>

using namespace std;

class compTecnicaEspecial{
    public:
    compTecnicaEspecial(){};
    virtual void tecnicaEspecial(){};
    ~compTecnicaEspecial(){};
};

class compGolpePatada{
    public:
    compGolpePatada(){};
    virtual void patada(){};
    ~compGolpePatada(){};
};

class compPoder{
    public:
    compPoder(){};
    virtual void poder(){};
    ~compPoder(){};
};

class Luchador{
    protected:
    compTecnicaEspecial* ptrTE;
    compGolpePatada* ptrGP;
    compPoder* ptrP;
    public:
    Luchador(){};
    virtual void saludo(){};
    void ejecutarEspecial(){ptrTE->tecnicaEspecial();}
    void ejecutarPatada(){ptrGP->patada();}
    void ejecutarPoder(){ptrP->poder();}
    ~Luchador(){};
    
};

class Jiraya: public compTecnicaEspecial,compGolpePatada,compPoder{
    Jiraya(){};
    void tecnicaEspecial(){cout<<"Jiraya Usa el Rasengan"<<endl;}
    void patada(){cout<<"Jiraya patea con fuerza"<<endl;}
    void poder(){cout<<"Jiraya invoca un sapo gigante"<<endl;}
    ~Jiraya(){};
};
class Kakashi: public compTecnicaEspecial,compGolpePatada,compPoder{
    Kakashi(){};
    void tecnicaEspecial(){cout<<"Kakashi usa el chidori"<<endl;}
    void patada(){cout<<"Kakashi patea rapidamente"<<endl;}
    void poder(){cout<<"Kakashi activa el sharingan"<<endl;}
    ~Kakashi(){};
};
class Minato: public compTecnicaEspecial,compGolpePatada,compPoder{
    Minato(){};
    void tecnicaEspecial(){cout<<"Minato usa teletransportaciÃ³n"<<endl;}
    void patada(){cout<<"Minato patea rapidamente"<<endl;}
    void poder(){cout<<"Minato activa el modo sabio"<<endl;}
    ~Minato(){};
};
int main()
{
    cout<<"Hello World";

    return 0;
}
